{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "org",
      "position",
      "startYear",
      "endYear",
      "ongoing",
      "details",
      "icon",
      "hide"
    ],
    "properties": {
      "org": { "type": "string" },
      "position": { "type": "string" },
      "startYear": { "type": "string" },
      "endYear": { "type": "string" },
      "ongoing": { "type": "boolean" },
      "details": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["point", "tags"],
          "properties": {
            "point": { "type": "string" },
            "tags": {
              "type": "array",
              "items": { "type": "string" }
            }
          },
          "additionalProperties": false
        }
      },
      "icon": {
        "oneOf": [
          {
            "type": "object",
            "required": ["src", "link"],
            "properties": {
              "src": { "type": "string" },
              "link": { "type": "string" }
            },
            "additionalProperties": false
          },
          { "type": "null" }
        ]
      },
      "hide": { "type": "boolean" }
    },
    "additionalProperties": false
  }
}
