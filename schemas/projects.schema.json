{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["title", "details", "images", "link", "hide"],
    "properties": {
      "title": { "type": "string" },
      "startYear": { "type": "string" },
      "endYear": { "type": "string" },
      "details": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["point", "tags"],
          "properties": {
            "point": { "type": "string" },
            "tags": {
              "type": "array",
              "items": { "type": "string" }
            }
          },
          "additionalProperties": false
        }
      },
      "images": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["src", "alt"],
          "properties": {
            "src": { "type": "string" },
            "alt": { "type": "string" }
          },
          "additionalProperties": false
        }
      },
      "link": {
        "type": "array",
        "items": {
          "type": "object",
          "required": ["url", "description"],
          "properties": {
            "url": { "type": "string" },
            "description": { "type": "string" }
          },
          "additionalProperties": false
        }
      },
      "hide": { "type": "boolean" }
    },
    "additionalProperties": false
  }
}
